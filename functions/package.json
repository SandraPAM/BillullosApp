    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "firebase-admin": "^12.1.1",
    "firebase-functions": "^5.0.1"
  },
  "devDependencies": {
    "typescript": "^5.4.5"
  },
  "private": true
}
